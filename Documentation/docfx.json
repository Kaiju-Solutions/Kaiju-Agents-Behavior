{
    "metadata": [
        {
            "src": [
                {
                    "src": "..",
                    "files": [
                        "Packages/**/*.cs"
                    ],
                    "exclude": [
                        "Assets/**/*.cs",
                    ]
                }
            ],
            "globalNamespaceId": "Global",
            "filter": "filterConfig.yml",
            "dest": "api",
            "enumSortOrder": "declaringOrder",
            "namespaceLayout": "nested"
        }
    ],
    "build": {
        "globalMetadata": {
            "_appTitle": "Kaiju Agents Behavior",
            "_appFooter": "<a href=\"/license\" href=\"License\">MIT License Â© 2026</a> <a href=\"https://kaijusolutions.ca\" title=\"Kaiju Solutions Inc.\">Kaiju Solutions Inc.</a>",
            "_enableSearch": true,
            "_disableContribution": true,
            "_appLogoPath": "logo.svg",
            "_appFaviconPath": "favicon.ico"
        },
        "content": [
            {
                "files": [
                    "toc.yml",
                    "index.md",
                    "license.md"
                ]
            },
            {
                "src": "api",
                "files": [
                    "*.yml"
                ],
                "dest": "api"
            },
            {
                "src": "manual",
                "files": [
                    "**/*.yml",
                    "**/*.md"
                ],
                "dest": "manual"
            },
            {
                "src": "contact",
                "files": [
                    "index.md"
                ],
                "dest": "contact"
            },
            {
                "src": "changelog",
                "files": [
                    "index.md"
                ],
                "dest": "changelog"
            },
            {
                "src": "license",
                "files": [
                    "index.md"
                ],
                "dest": "license"
            }
        ],
        "overwrite": [
            {
                "src": "..",
                "files": [
                    "Assets/Scripts/**/*.md",
                    "Documentation/namespaces/**/*.md"
                ]
            }
        ],
        "resource": [
            {
                "src": "resources",
                "files": [
                    "**/*"
                ]
            },
            {
                "src": "img",
                "files": [
                    "**/*"
                ],
                "dest": "img"
            }
        ],
        "sitemap": {
            "baseUrl": "https://behavior.kaijusolutions.ca",
            "changefreq": "weekly",
            "fileOptions": {
                "api/*": {
                    "changefreq": "daily"
                }
            }
        },
        "xref": [
            "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml"
        ],
        "xrefService": [
            "https://xref.docs.microsoft.com/query?uid={uid}"
        ],
        "dest": "../_site"
    }
}